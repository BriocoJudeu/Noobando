#include <iostream>
#include <iomanip>
using std::cout;
using std::cin;
using std::endl;

int main(){
int num, num1, num2;
cout << "- para subtracao, + para adicao, / para divisao e * para multiplicacao"
<< "Depois digite 2 numeros para calcular";
cin >> num >> num1 >> num2;

if ( num == + )
    cout << num1 + num2 << endl;
if ( num == / )
    cout << num1/num2 << endl;


    return 0;
}
