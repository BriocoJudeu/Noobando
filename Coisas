#include <iostream>
#include <iomanip>
using std::cout;
using std::cin;
using std::endl;

int main(){
int positivo, num1, num2;
cout << "- para subtracao, + para adicao, / para divisao e * para multiplicacao"
<< "Depois digite 2 numeros para calcular";
cin >> positivo >> num1 >> num2;

if ( positivo == + )
    cout << num1 + num2 << endl;
if ( positivo == / )
    cout << num1/num2 << endl;


    return 0;
}
